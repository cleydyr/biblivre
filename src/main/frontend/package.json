{
  "name": "biblivre-frontend",
  "version": "0.0.1",
  "main": "webpack.config.js",
  "scripts": {
    "start": "webpack serve --open",
    "build": "webpack --config webpack.config.js --mode production",
    "build:dev": "webpack --config webpack.config.js --mode development",
    "test": "jest",
    "test:watch": "jest --watch",
    "watch": "webpack --watch",
    "extract": "formatjs extract",
    "compile": "formatjs compile",
    "translations:extract:pt-BR": "yarn extract 'src/**/*.ts*' --ignore='**/*.d.ts' --out-file lang/pt-BR.json --id-interpolation-pattern '[sha512:contenthash:base64:6]'",
    "translations:compile:pt-BR": "yarn compile 'lang/pt-BR.json' --out-file compile-lang/pt-BR.json",
    "translations:rebuild:pt-BR": "yarn translations:extract:pt-BR && yarn translations:compile:pt-BR"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@elastic/datemath": "^5.0.3",
    "@elastic/eui": "^97.3.1",
    "@emotion/css": "^11.13.4",
    "@emotion/react": "^11.13.3",
    "@tanstack/react-query": "^5.36.2",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.3.0",
    "@uidotdev/usehooks": "^2.4.1",
    "moment": "^2.30.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-intl": "^6.8.7"
  },
  "devDependencies": {
    "@babel/core": "^7.24.5",
    "@babel/preset-env": "^7.24.5",
    "@babel/preset-react": "^7.24.1",
    "@babel/preset-typescript": "^7.24.1",
    "@eslint/js": "^9.14.0",
    "@formatjs/cli": "^6.3.8",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^15.0.7",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.14",
    "@typescript-eslint/eslint-plugin": "^8.13.0",
    "@typescript-eslint/parser": "^8.13.0",
    "babel-loader": "^9.1.3",
    "css-loader": "^7.1.1",
    "eslint": "^9.14.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-formatjs": "^5.2.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "file-loader": "^6.2.0",
    "globals": "^15.12.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "3.3.3",
    "style-loader": "^4.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "typescript-eslint": "^8.13.0",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.1.0"
  }
}
