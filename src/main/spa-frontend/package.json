{
  "name": "spa-frontend-2",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint . --cache",
    "lint:fix": "eslint . --fix --cache",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit --project tsconfig.app.json",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "extract": "formatjs extract",
    "compile": "formatjs compile",
    "i18n:extract-compile": "yarn extract \"src/**/*.{ts,tsx}\" --out-file lang.json --ignore \"src/**/*.d.ts\" && yarn compile lang.json --out-file lang-compiled.json"
  },
  "dependencies": {
    "@elastic/datemath": "^5.0.3",
    "@elastic/eui": "^106.3.0",
    "@elastic/eui-theme-borealis": "^3.3.1",
    "@emotion/css": "^11.13.5",
    "@emotion/react": "^11.14.0",
    "@tanstack/react-query": "^5.85.3",
    "@tanstack/react-query-devtools": "^5.87.4",
    "es-toolkit": "^1.39.10",
    "moment": "^2.30.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-intl": "^6.6.8",
    "react-router-dom": "^7.8.2"
  },
  "devDependencies": {
    "@elastic/eslint-plugin-eui": "^2.3.0",
    "@eslint/js": "^9.33.0",
    "@formatjs/cli": "^6.7.2",
    "@stylistic/eslint-plugin": "^5.3.1",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^15.0.7",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "@vitejs/plugin-react-swc": "^4.0.1",
    "eslint": "^9.33.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-intl": "^1.1.2",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globals": "^16.3.0",
    "jsdom": "^26.1.0",
    "prettier": "^3.6.2",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
    "utility-types": "^3.11.0",
    "vite": "^7.1.2",
    "vitest": "^3.2.4"
  }
}